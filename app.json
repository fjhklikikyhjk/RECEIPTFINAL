{
  "expo": {
    "name": "RAI",
    "slug": "receiptwise",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "receiptwise",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#00a5e9"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jaquallfulmore.receiptwise",
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.anonymous.RAI"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-secure-store",
      "@react-native-google-signin/google-signin",
      "expo-apple-authentication"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a3f41f58-b134-4345-a7d1-b69c8b87f3c8"
      }
    },
    "owner": "jaquall"
  }
}
